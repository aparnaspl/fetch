<?xml version="1.0" encoding="UTF-8"?>
<project name="MyProject" default="phpcs">
    <property name="phpcs"   value="phpcs"/>
    <property name="phpmd"   value="phpmd"/>
 
     <target name="phpcs">
    <exec executable="cmd">
        <arg line="/c phpcs
            --report=checkstyle
            --ignore='*.xml'
            --standard='PHPCS'
          
            -p
            test.php " />
    </exec>
</target>
    <target name="phpmd">
    <exec executable="cmd">
        <arg line="/c phpmd
            --report=checkstyle
            --ignore='*.xml'
            --standard='PHPCS'
          
            -p
            test.php " />
    </exec>
</target>
</project>
