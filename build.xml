<?xml version="1.0" encoding="UTF-8"?>
<project name="MyProject" default="phpcs">
<!--     <property name="phpcs"   value="phpcs"/> -->
    <property name="phpmd"   value="phpmd"/>

    <target name="phpmd">
    <exec executable="cmd">
        <arg line="/c phpmd
            --report=checkstyle
            --ignore='*.xml'
            --standard='PHPMD'
          
            -p
            test.php " />
    </exec>
</target>
</project>
