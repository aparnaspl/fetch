<?xml version="1.0" encoding="UTF-8"?>
<project name="MyProject" default="phpcs">
    <property name="phpcs"   value="phpcs"/>
 
     <target name="phpcs">
    <exec executable="cmd">
        <arg line="/c phpcs
            --report=checkstyle
            --report-file='${basedir}/build/logs/checkstyle.xml'
            --standard='PHPCS'
            --ignore=*\\lib\\Zend\\*
            -p
            test.php " />
    </exec>
</target>
</project>
